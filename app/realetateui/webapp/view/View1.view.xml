<mvc:View xmlns:unified="sap.ui.unified"
  controllerName="realetateui.controller.View1"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns="sap.m"
  displayBlock="true">

  <VBox id="uploadVBox" class="sapUiSmallMargin">
    <items>
      <Title id="uploadTitle" text="Upload Compound Image" level="H2" />

      <unified:FileUploader
        id="fileUploader"
        name="siteMap"
        uploadOnChange="false"
        change="onFileChange"
        fileType="jpg,jpeg,png"
        buttonText="Choose Image"
        width="300px" />

      <Button
        id="uploadBtn"
        text="Upload Image"
        press="onUploadPress"
        class="sapUiSmallMarginTop" />

      <Image
        id="siteMapImage"
        width="50%" height="50%" visible="false"  />
    </items>
  </VBox>
